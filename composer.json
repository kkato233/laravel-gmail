{
    "name": "kkato233/laravel-gmail",
    "description": "Laravel Mail Driver for Gmail API via Service Account",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/kkato233/laravel-gmail/issues",
        "source": "https://github.com/kkato233/laravel-gmail"
    },
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "google/apiclient": "^2.0",
        "google/apiclient-services": ">=0.200.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "kkato233\\LaravelGmail\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kkato233\\LaravelGmail\\LaravelGmailServiceProvider"
            ]
        }
    }
}
